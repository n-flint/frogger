<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
  <li>
    <%= link_to article.title, article_path(article), class: 'article-title', id: "article-#{article.id}" %>
  </li>
  <% end %>
</ul>

<%= link_to 'Create A New Article', new_article_path, class: 'new-article'%></br>
<%= link_to 'All Tags', tags_path, class: 'all-tags-link'%>